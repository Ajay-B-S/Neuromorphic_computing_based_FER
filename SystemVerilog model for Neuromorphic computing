int HAPPY, SAD, ANGRY, DISGUST, FEAR, SURPRISE;
int image\_number;
int any\_image;


initial begin
    \#1ps;
    V\_block = 5.0;

    regr\_cnt= 0;
    images\_cnt= images\_cnt + 1;

    image\_number = 0;
    repeat (30) begin // These iterations of training with NUM\_TRAIN\_IMAGES images of all emotions
        res\_cnt= 0;
        //images\_cnt= 0;
        image\_number = 0;
        \$display ("images\_cnt is \%d", images\_cnt);   
        HAPPY = 0; SAD = 0; ANGRY = 0; DISGUST = 0; FEAR = 0; SURPRISE = 0;
        repeat (NUM\_TRAIN\_IMAGES) begin // {
            \#1ps;
            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = happy\_image\_array[image\_number];
                //\$display ("Training HAPPY"); 

                \#100ps;
                if (I\_block[0] == 1 \&\& I\_block[1] == 0  \&\& I\_block[2] == 0  \&\& I\_block[3] == 0 \&\& I\_block[4] == 0  \&\& I\_block[5] == 0  ) begin 
                    res\_cnt = res\_cnt+1;
                    \$display ("found\_match HAPPY");   
                end else 
                    res\_cnt = res\_cnt;


            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = sad\_image\_array[image\_number];
                \#100ps;
                if (I\_block[0] == 0 \&\& I\_block[1] == 1  \&\& I\_block[2] == 0  \&\& I\_block[3] == 0 \&\& I\_block[4] == 0  \&\& I\_block[5] == 0   ) begin
                    res\_cnt = res\_cnt+1;
                    found\_match  = 1;
                    \$display ("found\_match SAD");   
                end else begin
                    res\_cnt = res\_cnt;
                end


            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = angry\_image\_array[image\_number];
                \#100ps;
                if (I\_block[0] == 0 \&\& I\_block[1] == 0  \&\& I\_block[2] == 1  \&\& I\_block[3] == 0 \&\& I\_block[4] == 0  \&\& I\_block[5] == 0  ) begin
                    res\_cnt = res\_cnt+1;
                    found\_match  = 1;
                    \$display ("found\_match ANGRY");   
                end else begin
                    res\_cnt = res\_cnt;
                end


            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = disgust\_image\_array[image\_number];
                \#100ps;
                if (I\_block[0] == 0 \&\& I\_block[1] == 0  \&\& I\_block[2] == 0  \&\& I\_block[3] == 1  \&\& I\_block[4] == 0  \&\& I\_block[5] == 0  ) begin
                    res\_cnt = res\_cnt+1;
                    found\_match  = 1;
                    \$display ("found\_match DISGUST");   
                end else begin
                    res\_cnt = res\_cnt;
                end


            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = fear\_image\_array[image\_number];
                \#100ps;
                if (I\_block[0] == 0 \&\& I\_block[1] == 0  \&\& I\_block[2] == 0  \&\& I\_block[3] == 0  \&\& I\_block[4] == 1  \&\& I\_block[5] == 0  ) begin
                    res\_cnt = res\_cnt+1;
                    found\_match  = 1;
                    \$display ("found\_match FEAR");   
                end else begin
                    res\_cnt = res\_cnt;
                end


            image\_number = \$urandom\_range(30,0);
                V\_block\_pixel\_value = surprise\_image\_array[image\_number];
                \#100ps;
                if (I\_block[0] == 0 \&\& I\_block[1] == 0  \&\& I\_block[2] == 0  \&\& I\_block[3] == 0  \&\& I\_block[4] == 0  \&\& I\_block[5] == 1  ) begin
                    res\_cnt = res\_cnt+1;
                    found\_match  = 1;
                    \$display ("found\_match SURPRISE");   
                end else begin
                    res\_cnt = res\_cnt;
                end

                           \#1ps;

        end // } // NUM\_TRAIN\_IMAGES
            images\_cnt = images\_cnt + 1;
            if (images\_cnt \% 6 == 0)
                regr\_cnt = regr\_cnt + 1;
            else 
                regr\_cnt =regr\_cnt;

            \#1ps;

        if (res\_cnt $>$ 20) begin
        end
    end

    \#100ps;
    \$finish();
end


        block blk\_00 (
            .V\_block (V\_block),
            .regr\_cnt (images\_cnt),
            .V\_block\_pixel\_value (V\_block\_pixel\_value),
            .Y\_layer5 (I\_block)
        );
endmodule
