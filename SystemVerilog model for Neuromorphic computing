% apdxb.tex

\chapter{Appendix: System Verilog Models for Neuromorphic computing}\label{apdx:b}
This is Appendix~\ref{apdx:b}.

import memristor\_pkg::*;
module memristor\_minus (I, V,G);
input bit V;
input bit G;
output Realsum\_NODE I;
///////////// Linear Ion Drift model ///////////////
//parameters definitions and default values
parameter real Roff = 200000;
parameter real Ron = 100;
parameter real D = 3;//nm
// local variables
parameter real w=0;
//parameter real G= (0.05);
real dwdt;
real R;
real W;
assign W = G * 3;
assign R=(Ron*(W/D)+Roff*(1-(W/D)));
assign I =  V*(1/R);
endmodule

module memristor\_plus (I, V,G);
input bit V;
input bit G;
output Realsum\_NODE I;
///////////// Linear Ion Drift model ///////////////
//parameters definitions and default values
parameter real Roff = 200000;
parameter real Ron = 100;
parameter real D = 3;//nm
// local variables
parameter real w=0;
//parameter real G= (0.05);
real dwdt;
real R;
real W;
assign W = G * 3;
assign R=(Ron*(W/D)+Roff*(1-(W/D)));
assign I =  V*(1/R);
endmodule


import memristor\_pkg::*;
module block (V\_block,regr\_cnt, V\_block\_pixel\_value,Y\_layer5);
parameter NUM\_INPUT\_PIXELS = 784;
parameter NUM\_NEURONS\_SPLIT\_1 = 9; 
parameter NUM\_NEURONS\_SPLIT\_2 = 9; 
parameter NUM\_NEURONS = (NUM\_NEURONS\_SPLIT\_1 * NUM\_NEURONS\_SPLIT\_2); //  neurons per layer
parameter NUM\_NEURONS\_PREV\_LAYER = (NUM\_NEURONS\_SPLIT\_1 * NUM\_NEURONS\_SPLIT\_2); //  neurons per previous layer
parameter NUM\_OUTPUT = 6; // HAPPY , SAD ... Total Emotion states

input integer regr\_cnt;
input real V\_block;
input bit [NUM\_INPUT\_PIXELS] V\_block\_pixel\_value ;
Realsum\_NODE I\_block\_plus\_layer1[NUM\_NEURONS];
Realsum\_NODE I\_block\_minus\_layer1[NUM\_NEURONS];
 bit Y\_layer1[NUM\_NEURONS-1:0];
bit [NUM\_INPUT\_PIXELS-1 : 0] binary\_weight\_plus\_layer1  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 
bit [NUM\_INPUT\_PIXELS-1 : 0] binary\_weight\_minus\_layer1  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 

Realsum\_NODE I\_block\_plus\_layer2[NUM\_NEURONS];
Realsum\_NODE I\_block\_minus\_layer2[NUM\_NEURONS];
 bit Y\_layer2[NUM\_NEURONS-1:0];
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_plus\_layer2  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2];  // 1024 neurons. Each neuron gets 1024 connections from previous layer
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_minus\_layer2  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 
Realsum\_NODE I\_block\_plus\_layer3[NUM\_NEURONS];
Realsum\_NODE I\_block\_minus\_layer3[NUM\_NEURONS];
 bit Y\_layer3[NUM\_NEURONS-1:0];
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_plus\_layer3  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_minus\_layer3  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 
Realsum\_NODE I\_block\_plus\_layer4[NUM\_NEURONS];
Realsum\_NODE I\_block\_minus\_layer4[NUM\_NEURONS];
 bit Y\_layer4[NUM\_NEURONS-1:0];
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_plus\_layer4  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_minus\_layer4  [NUM\_NEURONS\_SPLIT\_1 ] [NUM\_NEURONS\_SPLIT\_2]; 

Realsum\_NODE I\_block\_plus\_layer5[NUM\_OUTPUT];
Realsum\_NODE I\_block\_minus\_layer5[NUM\_OUTPUT];
output bit Y\_layer5[NUM\_OUTPUT-1:0];
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_plus\_layer5  [NUM\_OUTPUT]; 
bit [NUM\_NEURONS\_PREV\_LAYER-1 : 0] binary\_weight\_minus\_layer5  [NUM\_OUTPUT]; 

generate
genvar i,j,k,l;

    for (l=0;l<NUM\_NEURONS\_SPLIT\_1; l=l+1) begin
    for (j=0;j<NUM\_NEURONS\_SPLIT\_2; j=j+1) begin

        for (k =0; k<NUM\_INPUT\_PIXELS; k=k+1) begin
            always @ (regr\_cnt) begin
                binary\_weight\_plus\_layer1[l][j][k]  = \$urandom\_range(1,0);
                binary\_weight\_minus\_layer1[l][j][k] = \$urandom\_range(1,0);
                
            end
        end
        
        for (i=0; i<NUM\_INPUT\_PIXELS; i=i+1) begin
            memristor\_plus  mem\_g\_plus\_layer1 (
                .V (V\_block\_pixel\_value[i]),
                .I (I\_block\_plus\_layer1[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_plus\_layer1[l][j][i])
                );
        
            memristor\_minus  mem\_g\_minus\_layer1 (
                .V (V\_block\_pixel\_value[i]),
                .I (I\_block\_minus\_layer1[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_minus\_layer1[l][j][i])
                );
            
        end

            // Activation  function 
        assign \#1ps Y\_layer1[j + (NUM\_NEURONS\_SPLIT\_2 * l)] = ((I\_block\_plus\_layer1[j + (NUM\_NEURONS\_SPLIT\_2 * l)] - I\_block\_minus\_layer1[j + (NUM\_NEURONS\_SPLIT\_2 * l)]) $>$ 0) ? 1 : 0;

    end
    end

    for (l=0;l<NUM\_NEURONS\_SPLIT\_1; l=l+1) begin
    for (j=0;j<NUM\_NEURONS\_SPLIT\_2; j=j+1) begin
        for (k =0; k<NUM\_NEURONS; k=k+1) begin
            always @ (regr\_cnt) begin
                binary\_weight\_plus\_layer2[l][j][k]  = \$urandom\_range(1,0);
                binary\_weight\_minus\_layer2[l][j][k] = \$urandom\_range(1,0);
            end
        end
        for (i=0; i<NUM\_NEURONS; i=i+1) begin
            memristor\_plus  mem\_g\_plus\_layer2 (
                .V (Y\_layer1[i]),
                .I (I\_block\_plus\_layer2[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_plus\_layer2[l][j][i])
                );
            
            memristor\_minus  mem\_g\_minus\_layer2 (
                .V (Y\_layer1[i]),
                .I (I\_block\_minus\_layer2[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_minus\_layer2[l][j][i])
                );
            
        end

            // Activation  function 
        assign \#1ps Y\_layer2[j + (NUM\_NEURONS\_SPLIT\_2 * l)] = ((I\_block\_plus\_layer2[j + (NUM\_NEURONS\_SPLIT\_2 * l)] - I\_block\_minus\_layer2[j + (NUM\_NEURONS\_SPLIT\_2 * l)]) $>$ 0) ? 1 : 0;

    end
    end

    for (l=0;l<NUM\_NEURONS\_SPLIT\_1; l=l+1) begin
    for (j=0;j<NUM\_NEURONS\_SPLIT\_2; j=j+1) begin
        for (k =0; k<NUM\_NEURONS; k=k+1) begin
            always @ (regr\_cnt) begin
                binary\_weight\_plus\_layer3[l][j][k]  = \$urandom\_range(1,0);
                binary\_weight\_minus\_layer3[l][j][k] = \$urandom\_range(1,0);
            end
        end

        for (i=0; i<NUM\_NEURONS; i=i+1) begin
            memristor\_plus  mem\_g\_plus\_layer3 (
                .V (Y\_layer2[i]),
                .I (I\_block\_plus\_layer3[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_plus\_layer3[l][j][i])
                );
            
            memristor\_minus  mem\_g\_minus\_layer3 (
                .V (Y\_layer2[i]),
                .I (I\_block\_minus\_layer3[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_minus\_layer3[l][j][i])
                );
            
        end

            // Activation  function 
        assign \#1ps Y\_layer3[j + (NUM\_NEURONS\_SPLIT\_2 * l)] = ((I\_block\_plus\_layer3[j + (NUM\_NEURONS\_SPLIT\_2 * l)] - I\_block\_minus\_layer3[j + (NUM\_NEURONS\_SPLIT\_2 * l)]) $>$ 0) ? 1 : 0;

    end
    end

    for (l=0;l<NUM\_NEURONS\_SPLIT\_1; l=l+1) begin
    for (j=0;j<NUM\_NEURONS\_SPLIT\_2; j=j+1) begin

        for (k =0; k<NUM\_NEURONS; k=k+1) begin
            always @ (regr\_cnt) begin
                binary\_weight\_plus\_layer4[l][j][k]  = \$urandom\_range(1,0);
                binary\_weight\_minus\_layer4[l][j][k] = \$urandom\_range(1,0);
            end
        end

        for (i=0; i<NUM\_NEURONS; i=i+1) begin
            memristor\_plus  mem\_g\_plus\_layer3 (
                .V (Y\_layer3[i]),
                .I (I\_block\_plus\_layer4[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_plus\_layer4[l][j][i])
                );
            
            memristor\_minus  mem\_g\_minus\_layer3 (
                .V (Y\_layer3[i]),
                .I (I\_block\_minus\_layer4[j + (NUM\_NEURONS\_SPLIT\_2 * l)]),
                .G (binary\_weight\_minus\_layer4[l][j][i])
                );
            
        end

            // Activation  function 
        assign \#1ps Y\_layer4[j + (NUM\_NEURONS\_SPLIT\_2 * l)] = ((I\_block\_plus\_layer4[j + (NUM\_NEURONS\_SPLIT\_2 * l)] - I\_block\_minus\_layer4[j + (NUM\_NEURONS\_SPLIT\_2 * l)]) $>$ 0) ? 1 : 0;

    end
    end

    for (j=0;j<NUM\_OUTPUT; j=j+1) begin

        for (k =0; k<NUM\_NEURONS; k=k+1) begin
            always @ (regr\_cnt) begin
                binary\_weight\_plus\_layer5[j][k]  = \$urandom\_range(1,0);
                binary\_weight\_minus\_layer5[j][k] = \$urandom\_range(1,0);
            end
        end
    end

    for (j=0;j<NUM\_OUTPUT; j=j+1) begin
        for (i=0; i<NUM\_NEURONS; i=i+1) begin
            memristor\_plus  mem\_g\_plus\_layer3 (
                .V (Y\_layer4[i]),
                .I (I\_block\_plus\_layer5[j]),
                .G (binary\_weight\_plus\_layer5[j][i])
                );
            
            memristor\_minus  mem\_g\_minus\_layer3 (
                .V (Y\_layer4[i]),
                .I (I\_block\_minus\_layer5[j]),
                .G (binary\_weight\_minus\_layer5[j][i])
                );
            
        end
            // Activation  function 
        assign \#1ps Y\_layer5[j] = ((I\_block\_plus\_layer5[j] - I\_block\_minus\_layer5[j]) $>$ 0) ? 1 : 0;

    end
endgenerate
endmodule




module TB ();
parameter NUM\_INPUT\_PIXELS = 784;
parameter NUM\_TRAIN\_IMAGES = 30;
parameter NUM\_OUTPUT = 6; // HAPPY , SAD ... Emotion states

real V\_block;
bit I\_block[NUM\_OUTPUT-1:0];
bit [NUM\_INPUT\_PIXELS] V\_block\_pixel\_value ;
int cnt,regr\_cnt,images\_cnt,res\_cnt,found\_match;
bit [NUM\_INPUT\_PIXELS] happy\_image\_array [NUM\_TRAIN\_IMAGES] = '{
