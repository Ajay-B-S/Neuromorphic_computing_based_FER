bit [NUM\_INPUT\_PIXELS] sad\_image\_array [NUM\_TRAIN\_IMAGES] = '{

  'hc0ffe8111fff76ffffffd53801f3e7861edf7e0efeee7ff777dee7f5c7801b807dfb6b99ee7f91cf4ff8947fc2c0817816190d804000f0181066d210ff37e2c7df3e36007001a67bff347f9ea22136c05c060308200d447d0200142000003ff000ff
 
